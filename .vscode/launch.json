    "version": "0.2.0",
    "configurations": [
        {
            "name": "Eval mobilenet_0.5flops",
            "type": "python",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "program": "eval_mobilenet.py",
            "args": [
                "--model", "mobilenet_0.5flops",
                "--imagenet_path", "/data/dataset/imagenet/ilsvrc2012/torchvision/"
            ],
            "console": "integratedTerminal"
        },
        {
            "name": "Search mobilenet_0.5flops",
            "type": "python",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "program": "amc_search.py",
            "args": [
                "--job", "train",
                "--model", "mobilenet",
                "--dataset", "imagenet",
                "--preserve_ratio", "0.5",
                "--lbound", "0.2",
                "--rbound", "1",
                "--reward", "acc_reward",
                "--data_root", "/data/dataset/imagenet/ilsvrc2012/torchvision/",
                "--ckpt_path", "./checkpoints/mobilenet_imagenet.pth.tar",
                "--seed", "2018"
            ],
            "console": "integratedTerminal"
        }
    ]
}